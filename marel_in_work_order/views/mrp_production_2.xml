                            <field name="shift" />
                            <!-- <field name="no_box" /> -->
                            <!-- <field name="krono_kk_menit" />
                            <field name="jam_kerja" />
                            <field name="target_knitting"/>
                            <field name="target_conti"/>
                            <field name="target_as" />
                            <field name="target_sewing"/>
                            <field name="target_bordir"/>
                            <field name="target_setting"/> -->
                        </list>
                    </field>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
